{
    "name": "laravel-liberu/comments",
    "description": "Comments Manager for Laravel Enso",
    "keywords": [
        "laravel-liberu",
        "comments",
        "comments-manager",
        "comments"
    ],
    "homepage": "https://github.com/laravel-liberu/comments",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",
            "homepage": "https://laravel-liberu.com",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel-liberu/core": "^8.0",
        "laravel-liberu/dynamic-methods": "^2.0",
        "laravel-liberu/helpers": "^2.0",
        "laravel-liberu/migrator": "^2.1",
        "laravel-liberu/track-who": "^2.0"
    },
    "autoload": {
